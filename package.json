{
  "name": "metanno",
  "version": "0.0.8",
  "description": "Jupyter widgets collection to setup a modular annotation environment",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets",
    "annotation",
    "structured-data"
  ],
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/percevalw/metanno",
  "bugs": {
    "url": "https://github.com/percevalw/metanno/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Perceval Wajsburt",
    "email": ""
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/percevalw/metanno"
  },
  "scripts": {
    "build:labextension": "jlpm clean:labextension && jupyter labextension build .",
    "build:labextension:dev": "jlpm clean:labextension && jupyter labextension build --development True .",
    "build:lib:old": "jlpm run clean:lib && babel src --out-dir lib --copy-files -x .js,.jsx,.ts,.tsx && babel node_modules/react-data-grid --out-dir lib/node_modules/react-data-grid --copy-files -x .js && jlpm patch-rdg",
    "build:lib": "jlpm run clean:lib && babel src --out-dir lib --copy-files -x .js,.jsx,.ts,.tsx",
    "build": "jlpm run build:lib && jlpm run build:labextension",
    "build:dev": "jlpm run build:lib && jlpm run build:labextension:dev",
    "install-labextension": "jlpm run build && jlpm run install:prebuilt",
    "dev-start": "node scripts/start.js",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rm -rf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rm -rf metanno/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "peerDependencies": {
    "@jupyterlab/application": "3.2.4",
    "@jupyterlab/docregistry": "^3.0.0",
    "@jupyterlab/logconsole": "^3.0.0",
